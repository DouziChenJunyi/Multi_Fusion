     </el-table>\n                        </div>\n                      </template>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </el-aside>\n\n          <el-container width=\"100%\">\n            <el-aside width=\"39%\">\n              <div id=\"aside-right-top\" />\n              <div>\n\n                <div style=\"display: flex; justify-content: space-around; margin-top: 2%;\">\n                  <div id=\"btn_wrapper\" style=\"border: none;\">\n                    <el-button id=\"selectBiview_btn\" type=\"primary\" plain size=\"mini\" @click=\"click_to_selectBiview\">传入二路图</el-button>\n                  </div>\n                  <div id=\"btn_wrapper\" style=\"border: none;\">\n                    <el-button id=\"selectMulview_btn\" type=\"primary\" plain size=\"mini\" @click=\"click_to_selectMulview\">传入多视图</el-button>\n                  </div>\n                </div>\n                <input id=\"selectBiview_Input\" type=\"file\" style=\"display: none;\" multiple @change=\"selectBiview($event)\">\n                <input id=\"selectMulview_Input\" type=\"file\" style=\"display: none;\" multiple @change=\"selectMulview($event)\">\n                <div v-if=\"BivFusion_flag\" id=\"Bivcarousel\">\n                  <template>\n                    <div id=\"banner\">\n                      <!--动态将图片轮播图的容器高度设置成与图片一致-->\n                      <el-carousel :height=\"bannerHeight2 + 'px'\" :loop=\"true\" :autoplay=\"true\" trigger=\"click\">\n                        <!--遍历图片地址,动态生成轮播图-->\n                        <el-carousel-item v-for=\"item in send_Bivimg_list\" :key=\"item\">\n                          <img id=\"carousel_item_image\" :src=\"item\" alt=\"\">\n                        </el-carousel-item>\n                      </el-carousel>\n                    </div>\n                  </template>\n                </div>\n                <div v-if=\"MulvFusion_flag\" id=\"Mulvcarousel\">\n                  <template>\n                    <div id=\"banner\">\n                      <!--动态将图片轮播图的容器高度设置成与图片一致-->\n                      <el-carousel :height=\"bannerHeight2 + 'px'\" :loop=\"true\" :autoplay=\"true\" trigger=\"click\">\n                        <!--遍历图片地址,动态生成轮播图-->\n                        <el-carousel-item v-for=\"(item, index) in send_Mulvimg_list\" :key=\"index\">\n                          <img id=\"carousel_item_image\" :src=\"item\" alt=\"\">\n                        </el-carousel-item>\n                      </el-carousel>\n                    </div>\n                  </template>\n                </div>\n\n              </div>\n              <div id=\"btn_layer\">\n                <div style=\"display: flex; justify-content: space-around; margin-top: 2%;\">\n                  <div id=\"btn_wrapper\">\n                    <el-button id=\"BivFusion_btn\" plain size=\"mini\" @click=\"BivFusion\">二路图链接</el-button>\n                  </div>\n                  <div id=\"btn_wrapper\">\n                    <el-button id=\"MulvFusion_btn\" plain size=\"mini\" @click=\"MulvFusion\">多视图融合</el-button>\n                  </div>\n                </div>\n                <div id=\"\" style=\"height: 80%;\">\n                  <div style=\"display: flex; justify-content: space-around; margin-top: -30%;\">\n                    <div id=\"popover_wrapper\">\n                      <div style=\"display: flex; justify-content: space-around;\">\n                        <template>\n                          <el-popover\n                            placement=\"top\"\n                            title=\"\"\n                            width=\"200\"\n                            trigger=\"click\"\n                            content=\"二路图链接指的是二路链接,多视图融合指的是多个视图融合\"\n                          >\n                            <el-button id=\"Text_btn\" slot=\"reference\" type=\"info\" round>简介显示</el-button>\n                          </el-popover>\n                        </template>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n            </el-aside>\n            <el-main>\n              <div id=\"main\">\n                <div id=\"result-head\" width=\"80%\" height=\"100%\">\n                  <div style=\"display: flex; margin-left: 34%;\">\n                    <img id=\"result_text\" src=\"../assets/result.png\">\n                  </div>\n                </div>\n                <div id=\"sptsrv_wrapper2\">\n                  <div id=\"sptsrv-table\">\n                    <template>\n                      <div class=\"table-wrapper\">\n                        <el-table\n                          :row-style=\"getRowClass\"\n                          :data=\"tableData\"\n                          :show-header=\"false\"\n                          :header-row-style=\"getHeaderRowClass\"\n                          :max-height=\"100\"\n                          :cell-style=\"getCellClass\"\n                        >\n                          <el-table-column\n                            :row-style=\"getRowClass\"\n                            label=\"序号\"\n                            prop=\"list\"\n                          />\n                          <el-table-column\n                            label=\"序号\"\n                            prop=\"list\"\n                            width=\"100%\"\n                          />\n                          <el-table-column\n                            label=\"序号\"\n                            prop=\"list\"\n                            width=\"100%\"\n                          />\n                          <el-table-column\n                            label=\"序号\"\n                            prop=\"list\"\n                            width=\"100%\"\n                          />\n                        </el-table>\n                      </div>\n                    </template>\n                  </div>\n                </div>\n                <div id=\"result_image\">\n                  <div id=\"result-head\" width=\"80%\" height=\"100%\">\n                    <div style=\"display: flex; margin-left: 32.5%;\">\n                      <img id=\"result_img_text\" src=\"../assets/result_img_text1.png\">\n                    </div>\n                  </div>\n                  <div id=\"carousel_wrapper\" style=\"width:90%;margin: -4% 0 0 5.5%;\">\n                    <div v-if=\"MulvFusion_flag\" id=\"carousel\">\n                      <template>\n                        <div id=\"banner\">\n                          <!--动态将图片轮播图的容器高度设置成与图片一致-->\n                          <el-carousel :height=\"bannerHeight + 'px'\" trigger=\"click\">\n                            <!--遍历图片地址,动态生成轮播图-->\n                            <el-carousel-item v-for=\"(item, index) in get_Mulvimg_list\" :key=\"index\">\n                              <img id=\"carousel_item_image\" :src=\"item\" alt=\"\">\n                            </el-carousel-item>\n                          </el-carousel>\n                        </div>\n                      </template>\n                    </div>\n                    <div v-if=\"BivFusion_flag\" id=\"carousel\">\n                      <template>\n                        <div id=\"banner\">\n                          <!--动态将图片轮播图的容器高度设置成与图片一致-->\n                          <el-carousel :height=\"bannerHeight + 'px'\" trigger=\"click\">\n                            <!--遍历图片地址,动态生成轮播图-->\n                            <el-carousel-item v-for=\"(item, index) in get_Bivimg_list\" :key=\"index\">\n                              <img id=\"carousel_item_image\" :src=\"item\" alt=\"\">\n                            </el-carousel-item>\n                          </el-carousel>\n                        </div>\n                      </template>\n                    </div>\n                  </div>\n                </div>\n\n                <div style=\"margin: -10% 0 0 5%;\">\n                  <div id=\"timecnt\" style=\"margin-top: 5%; display: flex;\">\n                    <div id=\"timecnt-text\">识别时间:</div>\n                    <div id=\"timecnt_boxlayer\" style=\"display:inline-block; margin-top: -10.5%;\">\n                      <div id=\"number_box\">8</div>\n                      <div id=\"number_box\">.</div>\n                      <div id=\"number_box\">1</div>\n                      <div id=\"number_box\">1</div>\n                      <div id=\"number_box\">1</div>\n                      <div id=\"number_box\">S</div>\n                    </div>\n                  </div>\n                </div>\n\n              </div>\n            </el-main>\n          </el-container>\n        </el-container>\n      </el-container>\n    </div>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    return {\n      BivFusion_flag: false,\n      MulvFusion_flag: true,\n      request_form: {},\n      tableData: [{\n        list: '已启动'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }, {\n        list: '已启动服务1'\n      }],\n      info_box_text: '这是多模态数据融合，其功能是进行可文本数据源及可见光图像数据源的融合处理。这是多模态数据融合，其功能是进行。',\n      send_Bivimg_list: [\n        require('../assets/click_sign.png'),\n        require('../assets/1fog_ir_InfraRedDetection.jpg')\n      ],\n      send_Mulvimg_list: [\n        require('../assets/no_result.png'),\n        require('../assets/1fog_ir_InfraRedDetection.jpg'),\n        require('../assets/1fog_ir.jpg'),\n        require('../assets/1fog_uav_Confusion.jpg'),\n        require('../assets/1fog_uav_UAVDetection.jpg'),\n        require('../assets/1fog_uav.jpg'),\n        require('../assets/click_sign.png'),\n        require('../assets/earth.png')\n      ],\n      get_Bivimg_list: [\n        require('../assets/no_result.png'),\n        require('../assets/click_sign.png')\n      ],\n      get_Mulvimg_list: [\n        require('../assets/no_result.png'),\n        require('../assets/1fog_ir_InfraRedDetection.jpg'),\n        require('../assets/1fog_ir.jpg'),\n        require('../assets/1fog_uav_Confusion.jpg'),\n        require('../assets/1fog_uav_UAVDetection.jpg'),\n        require('../assets/1fog_uav.jpg'),\n        require('../assets/click_sign.png'),\n        require('../assets/earth.png')\n      ],\n      // 图片父容器高度\n      bannerHeight: 1000,\n      bannerHeight2: 150,\n      // 浏览器宽度\n      screenWidth: 0\n    }\n  },\n  mounted() {\n    // 首次加载时,需要调用一次\n    this.screenWidth = window.innerWidth\n    this.setSize()\n    // 窗口大小发生改变时,调用一次\n    window.onresize = () => {\n      this.screenWidth = window.innerWidth\n      this.setSize()\n    }\n  },\n  created() {\n    this.request_form = new FormData()\n  },\n  methods: {\n    deleteRow(index, rows) {\n      rows.splice(index, 1)\n    },\n    getCellClass() {\n      const styleJson = {\n        'text-align': 'centerjjjj',\n        'border': 'none'\n      }\n      return styleJson\n    },\n    getRowClass({ row, column, rowIndex, columnIndex }) {\n      const styleJson = {\n        'color': '#FFFFFF',\n        'font-weight': 'bold',\n        'font-size': '0.8rem',\n        'width': '80%',\n        'background-color': '#030c60'\n      }\n      return styleJson\n    },\n    getHeaderRowClass() {\n      const styleJson = {\n        'height': '1rem'\n      }\n      return styleJson\n    },\n    nav_multisrc() {\n      this.$router.replace({\n        path: '/MultiSource',\n        query: {\n          t: +new Date()\n        }\n      })\n    },\n    nav_multimodel() {\n      this.$router.replace({\n        path: '/MultiModel',\n        query: {\n          t: +new Date()\n        }\n      })\n    },\n    nav_multiview() {\n      this.$router.replace({\n        path: '/MultiView',\n        query: {\n          t: +new Date()\n        }\n      })\n    },\n    c