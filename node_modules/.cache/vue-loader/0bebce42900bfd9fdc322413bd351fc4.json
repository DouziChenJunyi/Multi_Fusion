srv-table\">\n                      <template>\n                        <div class=\"table-wrapper\">\n                          <el-table\n                            :row-style=\"getRowClass\"\n                            :data=\"tableData\"\n                            :show-header=\"false\"\n                            :header-row-style=\"getHeaderRowClass\"\n                            :max-height=\"100\"\n                            :cell-style=\"getCellClass\"\n                          >\n                            <el-table-column\n                              :row-style=\"getRowClass\"\n                              label=\"序号\"\n                              prop=\"list\"\n                            />\n                            <el-table-column\n                              label=\"序号\"\n                              prop=\"list\"\n                              width=\"100%\"\n                            />\n                            <el-table-column\n                              label=\"序号\"\n                              prop=\"list\"\n                              width=\"100%\"\n                            />\n                            <el-table-column\n                              label=\"序号\"\n                              prop=\"list\"\n                              width=\"100%\"\n                            />\n                          </el-table>\n                        </div>\n                      </template>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </el-aside>\n\n          <el-container width=\"100%\">\n            <el-aside width=\"50%\">\n              <div id=\"aside-right-top\" />\n              <div width=\"100%\" height=\"60%\">\n                <div id=\"carousel\">\n                  <template>\n                    <el-carousel>\n                      <el-carousel-item v-for=\"item in img_list\" :key=\"item\">\n                    